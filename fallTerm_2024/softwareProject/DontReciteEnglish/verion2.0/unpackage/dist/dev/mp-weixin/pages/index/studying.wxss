
.funButton{
		
		width: 480rpx;
		height: 80rpx;
		border-radius: 30rpx;
		line-height: 80rpx;
		text-align: center;
		font-size: 36rpx;
		font-weight: 500;
}
.doIt{
		
		width: 580rpx;
		height: 540rpx;
		
		margin-top: 350rpx;
		z-index: 5;
		display: flex;
		flex-direction: column;
		justify-content: space-around;
}
.inputButton{
	position: absolute;
	left: 80rpx;
	width: 410rpx;
	height: 50rpx;
	top:15rpx;
}
.search{
		width: 500rpx;
		height: 300rpx;
		position: absolute;
		top: 15rpx;
		left: 20rpx;
}
.paper{
		position: absolute;
		width: 620rpx;
		height: 1100rpx;
		top: 60rpx;
		left: 65rpx;
		z-index: -1;
}
.funBox{
		width: 500rpx;
		height: 1100rpx;
		margin-left: 130rpx;
		margin-top: 100rpx;
		overflow: hidden;
		z-index: 5;
		position: relative;
}
.barBox{
		margin-top: 5rpx;
		height: 45rpx;
		width: 45rpx;
}
.barBox image{
		height: 45rpx;
		width: 45rpx;
}
.myBar{
		position: absolute;
		margin-left: 50rpx;
		width: 650rpx;
		height: 55rpx;
		display: flex;
		justify-content: space-between;
		bottom: 35rpx;
		z-index: 5;
}
.main{
		position: relative;
		margin-top: 100rpx;
		width: 750rpx;
		height: 1500rpx;
		border-radius: 80rpx;
		background-color: antiquewhite;
		z-index: 2;
		overflow: hidden;
}
.topImage{
		margin: 20rpx 30rpx;
		width: 50rpx;
		height: 50rpx;
}
.bottomBg image{
		margin-left: 40rpx;
		width: 680rpx;
		height: 130rpx;
		z-index: 1;
		-webkit-transform: scale(1.2);
		        transform: scale(1.2);
}
.bottomBg{
		width: 750rpx;
		height: 150rpx;
		position: absolute;
		bottom: 0rpx;
		z-index: 3;
}
.sc{
		position: absolute;
		width: 750rpx;
		height: 100rpx;
		z-index: 2;
		display: flex;
		justify-content: space-between;
}
.topBg{
		width: 750rpx;
		height: 900rpx;
		position: absolute;
		top: 0;
		z-index: 1;
}
.topBg image{
		width: 750rpx;
		height: 1100rpx;
		-webkit-transform: translate(0rpx, -470rpx);
		        transform: translate(0rpx, -470rpx);
		z-index: 1;
}
.container{
		width: 750rpx;
		height: 1445rpx;
		position: relative;
		overflow: hidden;
		z-index: 1;
}

