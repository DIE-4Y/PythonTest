
.barBox{
	margin-top: 5rpx;
	height: 45rpx;
	width: 45rpx;
}
.barBox image{
	height: 45rpx;
	width: 45rpx;
}
.myBar{
	position: absolute;
	margin-left: 50rpx;
	width: 650rpx;
	height: 55rpx;
	display: flex;
	justify-content: space-between;
	bottom: 35rpx;
	z-index: 5;
}
.main image{
	
	width: 750rpx;
	height: 1100rpx;
	opacity: 0.8;
}
.main{
	position: relative;
	margin-top: 100rpx;
	width: 750rpx;
	height: 1215rpx;
	border-radius: 80rpx;
	z-index: 2;
	overflow: hidden;
}
.topImage{
	margin: 20rpx 30rpx;
	width: 50rpx;
	height: 50rpx;
}
.bottomBg image{
	margin-left: 40rpx;
	width: 680rpx;
	height: 130rpx;
	z-index: 1;
	-webkit-transform: scale(1.2);
	        transform: scale(1.2);
}
.bottomBg{
	width: 750rpx;
	height: 150rpx;
	position: absolute;
	bottom: 0rpx;
	z-index: 3;
}
.sc{
	position: absolute;
	width: 750rpx;
	height: 100rpx;
	z-index: 2;
	display: flex;
	justify-content: space-between;
}
.topBg{
	width: 750rpx;
	height: 900rpx;
	position: absolute;
	top: 0;
	z-index: 1;
}
.topBg image{
	width: 750rpx;
	height: 1270rpx;
	/* transform: translate(0rpx, -470rpx); */
	z-index: 1;
	opacity: 0.5;
}
.container{
	width: 750rpx;
	height: 1445rpx;
	position: relative;
	overflow: hidden;
	z-index: 1;
	background-color: rgb(250, 244, 228);
}

